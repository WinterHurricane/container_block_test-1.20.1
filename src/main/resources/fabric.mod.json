	"entrypoints": {
		"main": [
			"net.winterhurricane.kio.KIO"
		],
		"fabric-datagen": [
			"net.winterhurricane.kio.KIODataGenerator"
		],
		"client": [
			"net.winterhurricane.kio.KIOClient"
		]
	},
	"mixins": [
		"kio.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.22",
